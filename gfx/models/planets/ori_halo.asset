# 半圆框架1,无旋转
entity = {
    name = "haloworld_full_construction_entity_01"
    cull_radius = 500.0
    scale = 0.8


    default_state = "idle"
    state = { name = "idle"
        start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_02" stop_on_state_change = yes } }
    }

    state = { name = "construction"
        start_event = { trigger_once = yes sound = { soundeffect = "ringworld_construction_ambient" } }
    }

    # 12个部分，每个占30度
    # 半径设为450（与你原来的保持一致）
    # 每个部分的旋转角度 = 该部分的位置角度

    # 0度位置（12点钟方向）
    locator = { name = "locator1"
        position = { 0  0  450 }
        rotation = { 0 0 0 }
    }

    # 30度位置（1点钟方向）
    locator = { name = "locator2"
        position = { 225  0  389.71 }
        rotation = { 30 0 0 }
    }

    # 60度位置（2点钟方向）
    locator = { name = "locator3"
        position = { 389.71  0  225 }
        rotation = { 60 0 0 }
    }

    # 90度位置（3点钟方向）
    locator = { name = "locator4"
        position = { 450  0  0 }
        rotation = { 90 0 0 }
    }

    # 120度位置（4点钟方向）
    locator = { name = "locator5"
        position = { 389.71  0  -225 }
        rotation = { 120 0 0 }
    }

    # 150度位置（5点钟方向）
    locator = { name = "locator6"
        position = { 225  0  -389.71 }
        rotation = { 150 0 0 }
    }


    # 附加12个construction实体
    attach = { locator1 = "ringworld_construction_entity" }
    attach = { locator2 = "ringworld_construction_entity" }
    attach = { locator3 = "ringworld_construction_entity" }
    attach = { locator4 = "ringworld_construction_entity" }
    attach = { locator5 = "ringworld_construction_entity" }
    attach = { locator6 = "ringworld_construction_entity" }
}
# 半圆框架2,无旋转
entity = {
    name = "haloworld_full_construction_entity_02"
    cull_radius = 500.0
    scale = 0.8


    default_state = "idle"
    state = { name = "idle"
        start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_02" stop_on_state_change = yes } }
    }

    state = { name = "construction"
        start_event = { trigger_once = yes sound = { soundeffect = "ringworld_construction_ambient" } }
    }

    # 12个部分，每个占30度
    # 半径设为450（与你原来的保持一致）
    # 每个部分的旋转角度 = 该部分的位置角度

    # 180度位置（6点钟方向）
    locator = { name = "locator1"
        position = { 0  0  -450 }
        rotation = { 180 0 0 }
    }

    # 210度位置（7点钟方向）
    locator = { name = "locator2"
        position = { -225  0  -389.71 }
        rotation = { -150 0 0 }  # 或 210 0 0
    }

    # 240度位置（8点钟方向）
    locator = { name = "locator3"
        position = { -389.71  0  -225 }
        rotation = { -120 0 0 }  # 或 240 0 0
    }

    # 270度位置（9点钟方向）
    locator = { name = "locator4"
        position = { -450  0  0 }
        rotation = { -90 0 0 }  # 或 270 0 0
    }

    # 300度位置（10点钟方向）
    locator = { name = "locator5"
        position = { -389.71  0  225 }
        rotation = { -60 0 0 }  # 或 300 0 0
    }

    # 330度位置（11点钟方向）
    locator = { name = "locator6"
        position = { -225  0  389.71 }
        rotation = { -30 0 0 }  # 或 330 0 0
    }

    # 附加12个construction实体
    attach = { locator1 = "ringworld_construction_entity" }
    attach = { locator2 = "ringworld_construction_entity" }
    attach = { locator3 = "ringworld_construction_entity" }
    attach = { locator4 = "ringworld_construction_entity" }
    attach = { locator5 = "ringworld_construction_entity" }
    attach = { locator6 = "ringworld_construction_entity" }
}

